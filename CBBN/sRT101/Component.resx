  <data name="RT10121.CommandText" xml:space="preserve">
    <value>SELECT     A.comq1, 
A.lineq1,A.PRTNO, A.seq, A.SEQ2, A.CHGDAT, 
RTCode.CODENC, RTObj.CUSNC, RTProdH.PRODNC+'--'+ RTProdD1.SPEC, A.QTY, A.DROPDAT, 
HBwarehouse.WARENAME, A.ASSETNO,A.BATCHNO,A.TARDAT 
FROM RTProdD1 RIGHT OUTER JOIN RTLessorAVSCmtyLineHARDWARELOG A LEFT OUTER JOIN 
HBwarehouse ON A.WAREHOUSE = HBwarehouse.WAREHOUSE ON  RTProdD1.PRODNO = A.PRODNO AND 
RTProdD1.ITEMNO = A.ITEMNO LEFT OUTER JOIN RTProdH ON A.PRODNO = RTProdH.PRODNO 
LEFT OUTER JOIN RTCode ON A.CHGCODE = RTCode.CODE AND RTCode.KIND = 'G2' LEFT OUTER JOIN RTObj INNER JOIN 
RTEmployee ON RTObj.CUSID = RTEmployee.CUSID ON A.CHGUsR = RTEmployee.EMPLY </value>
  </data>
