  <data name="RTLessorAVSCmtyH.CommandText" xml:space="preserve">
    <value>SELECT A.* 
, (SELECT COUNT(B.LINEQ1) AS QT_LINE FROM RTLessorAVSCmtyLine B WHERE ISNULL(DROPDAT,'')='' AND ISNULL(CANCELDAT, '') = '' AND COMQ1=A.COMQ1) AS QT_LINE
, B.QT_CUST, C.CUTNC+A.TOWNSHIP+A.RADDR AS ADDR
FROM RTLessorAVSCmtyH A
LEFT JOIN  (SELECT COMQ1, COUNT(B.CUSID) AS QT_CUST 
FROM RTLessorAVSCust B WHERE ISNULL(DROPDAT,'')='' AND ISNULL(CANCELDAT, '') = '' 
GROUP BY COMQ1) B ON B.COMQ1 = A.COMQ1
LEFT JOIN RTCounty C ON C.CUTID=A.CUTID
</value>
  </data>
