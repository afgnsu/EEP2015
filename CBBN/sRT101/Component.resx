  <data name="RT1012.CommandText" xml:space="preserve">
    <value>SELECT A.* FROM (
SELECT A.comq1,A.lineq1, 
'' AS FAQNO,A.PRTNO, A.seq,'裝機' AS KD,B.SENDWORKDAT, 
RTProdH.PRODNC + '--' + RTProdD1.itemnc + '('+ RTProdD1.SPEC+')' AS PROD, A.QTY, A.amt, 
HBwarehouse.WARENAME, A.DROPDAT, A.DROPREASON, RTObj.CUSNC,A.BATCHNO,A.TARDAT,A.rcvprtno,A.rcvfinishdat  
FROM RTProdH RIGHT OUTER JOIN RTLessorAVSCmtyLineHARDWARE A LEFT OUTER JOIN 
RTObj INNER JOIN RTEmployee ON RTObj.CUSID = RTEmployee.CUSID ON 
A.DROPUSR = RTEmployee.EMPLY LEFT OUTER JOIN 
HBwarehouse ON A.WAREHOUSE = HBwarehouse.WAREHOUSE LEFT OUTER 
JOIN RTProdD1 ON A.PRODNO = RTProdD1.PRODNO AND 
A.ITEMNO = RTProdD1.ITEMNO ON RTProdH.PRODNO = A.PRODNO 
LEFT OUTER JOIN RTLessorAVSCmtyLinesndwork B on A.comq1=B.comq1 and A.lineq1=B.lineq1 and A.prtno=B.prtno 
WHERE 1=1
 union ALL
SELECT A.comq1,A.lineq1, 
A.FAQNO,A.PRTNO, A.seq,'維修' AS KD,B.SENDWORKDAT, 
RTProdH.PRODNC + '--' + RTProdD1.itemnc + '('+ RTProdD1.SPEC+')'  AS PROD, A.QTY, A.amt, 
HBwarehouse.WARENAME, A.DROPDAT, A.DROPREASON, RTObj.CUSNC,A.BATCHNO,A.TARDAT,A.rcvprtno,A.rcvfinishdat  
FROM RTProdH RIGHT OUTER JOIN RTLessorAVSCmtyLinefaqHARDWARE A LEFT OUTER JOIN 
RTObj INNER JOIN RTEmployee ON RTObj.CUSID = RTEmployee.CUSID ON 
A.DROPUSR = RTEmployee.EMPLY LEFT OUTER JOIN 
HBwarehouse ON A.WAREHOUSE = HBwarehouse.WAREHOUSE LEFT OUTER 
JOIN RTProdD1 ON A.PRODNO = RTProdD1.PRODNO AND 
A.ITEMNO = RTProdD1.ITEMNO ON RTProdH.PRODNO = A.PRODNO 
LEFT OUTER JOIN RTLessorAVSCmtyLinefaqsndwork B on A.comq1=B.comq1 and A.lineq1=B.lineq1 and A.faqno=B.faqno and A.prtno=B.prtno 
WHERE 1=1) A
order by rcvfinishdat </value>
  </data>
